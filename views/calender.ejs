<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("partials/head")%>
    <link rel="stylesheet" href="../css/calender.css">
    <link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
    <link rel='stylesheet prefetch' href='http://netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css'>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200">

    <title>RudeTrader</title>
    
    <style></style>
  </head>

  <body>
    

    <!-- this is taskbar which includes logo, dashboard,chalander -->
    <%- include("partials/taskbar")%>
    
    <!-- add trade button -->
    <%- include("partials/add-trade")%>

    <!--main web page -->
    <div class="main" id="main">
      <div class="main-header">
        <div class="page-name">Chalender </div>
        <div class="color-addtrade-button">
          <button class="header-button" id="add-trade-button">Add Trade</button>
        </div>
        
        <!-- profile dropdown-menu -->
        <%- include("partials/profile-dropdown-menu")%>
      </div>


      <!--main contant-->
      <div class="main-contant">

       
    <div class="wrapper">
      <header>
        <p class="current-date"></p>
        <div class="icons">
          <span id="prev" class="material-symbols-rounded">chevron_left</span>
          <span id="next" class="material-symbols-rounded">chevron_right</span>
        </div>
      </header>
      <div class="calendar">
        <ul class="weeks">
          <li>Sun</li>
          <li>Mon</li>
          <li>Tue</li>
          <li>Wed</li>
          <li>Thu</li>
          <li>Fri</li>
          <li>Sat</li>
        </ul>
        <ul class="days"></ul>
      </div>
    </div>
    

       


      </div>
      <!--main contant ends -->
    </div>
    <!--main webpage ends-->

    <script src="../js/layout.js">
      


    </script>
    
  <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src="../js/calender.js"></script>
  </body>
</html>
